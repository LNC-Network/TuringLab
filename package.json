{
  "name": "TuringLab",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "rm -rf node_modules apps/*/node_modules apps/*/.next apps/*/.turbo",
    "clean:build": "rm -rf apps/*/.next apps/*/.turbo",
    "setup": "pnpm install && cp apps/backend/.env.example apps/backend/.env",
    "dev:frontend": "cd apps/frontend && pnpm dev",
    "dev:backend": "cd apps/backend && pnpm dev",
    "build:frontend": "cd apps/frontend && pnpm build",
    "build:backend": "cd apps/backend && pnpm build"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
